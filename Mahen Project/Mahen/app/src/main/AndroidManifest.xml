<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mona.mahen">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.SEND_SMS" />


    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.Front">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".UI.Registration" />
        <activity android:name=".UI.Login" />
        <activity android:name=".UI.Selection_worker" />
        <activity android:name=".UI.Propic" />
        <activity android:name=".Account.MainActivity">
            <activity
                android:name=".Account.MapsActivity"
                android:label="@string/title_activity_maps" />
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        android:label="@string/title_activity_worker__maps" />
        <activity android:name=".Account.MapsActivity" />
        <activity android:name=".Worker.Main" />
        <activity android:name=".UI.Invoice" />
        <activity android:name=".UI.Worker_Propic" />
        <activity android:name=".Account.Posted_Work_Worker_Type" />
        <activity android:name=".Account.Post_Work" />
        <activity android:name=".Worker.Apply_For_Work" />
        <activity android:name=".Worker.Apply_For_Work_Details" />
        <activity android:name=".Account.Set_wage" />
        <activity android:name=".Account.Give_Rating" />
        <activity android:name=".Account.RateAndPay" />
        <activity android:name=".Account.See_Applicant" />
        <activity android:name=".Account.Work_Status" />
        <activity android:name=".Account.Work_Status_Details" />
        <activity android:name=".UI.Workermap" />
        <activity android:name=".Worker.Set_Wage" />
        <activity android:name=".Worker.My_Work" />
        <activity android:name=".Worker.Work_Done"></activity>
    </application>
</manifest>